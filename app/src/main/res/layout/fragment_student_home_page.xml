<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".student.studentHomePage.StudentHomePage">

    <ImageView
        android:id="@+id/StuCFlogo"
        android:layout_width="45dp"
        android:layout_height="45dp"

        android:src="@drawable/logoclassflow"
        android:background="@drawable/ic_input_icbg"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>



    <TextView
        android:id="@+id/StuHomeHeading"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="Home"
        android:textSize="20sp"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:textStyle="bold"
        android:gravity="center"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:src="@drawable/college_image"
        app:layout_constraintTop_toBottomOf="@id/StuHomeHeading"/>










</androidx.constraintlayout.widget.ConstraintLayout>